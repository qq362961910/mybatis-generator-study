<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
    PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
    "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

    <!-- 引入配置文件 -->
    <properties resource="jdbc.properties" />

    <context id="generateTables" targetRuntime="MyBatis3">

        <!-- 配置内置的或者自定义的Plugin -->
        <plugin type="plugin.MapperPlugin" />


        <commentGenerator>
            <property name="suppressAllComments" value="true" />
        </commentGenerator>
        <!--数据库连接的信息：驱动类、连接地址、用户名、密码 -->
        <jdbcConnection driverClass="${jdbc.driverClassName}"
                        connectionURL="${jdbc.url}"
                        userId="${jdbc.username}"
                        password="${jdbc.password}">
        </jdbcConnection>

        <javaModelGenerator targetPackage="com.yuntong.backend.entity"
                            targetProject="src/main/java">
            <property name="trimStrings" value="true" />
            <property name="rootClass" value="com.study.entity.BaseEntity"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mybatis.mapping"
                         targetProject="src/main/resources">
        </sqlMapGenerator>

        <!-- targetPackage：mapper接口生成的位置 -->
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.yuntong.backend.dao"
                             targetProject="src/main/java">
        </javaClientGenerator>

        <!-- 指定数据库表 -->
        <!--<table tableName="yt_user" domainObjectName="UserBase" />-->
        <!--<table tableName="yt_role" domainObjectName="RoleBase" />-->
        <!--<table tableName="yt_user_role" domainObjectName="UserRoleBase" />-->
        <!--<table tableName="yt_uri_source" domainObjectName="UriSourceBase" />-->
        <!--<table tableName="yt_role_source" domainObjectName="RoleSourceBase" />-->
        <!--<table tableName="yt_sys_log" domainObjectName="SysLogBase" />-->
        <!--<table tableName="yt_storage" domainObjectName="StorageBase" />-->
        <table tableName="yt_contract" domainObjectName="ContractBase" />
        <!--<table tableName="yt_contract_order" domainObjectName="ContractOrderBase" />-->
        <!--<table tableName="yt_small_item" domainObjectName="SmallItemBase" />-->
        <!--<table tableName="yt_small_item_attachment" domainObjectName="SmallItemAttachmentBase" />-->
        <!--<table tableName="yt_material" domainObjectName="MaterialBase" />-->
        <!--<table tableName="yt_material_log" domainObjectName="MaterialLogBase" />-->

    </context>
</generatorConfiguration>
